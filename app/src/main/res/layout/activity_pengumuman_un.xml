<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.PengumumanUnActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Hasil Ujian Nasional"
            android:textAlignment="center"
            android:textSize="36dp"
            android:textStyle="bold"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginVertical="3dp"
            android:layout_marginHorizontal="4dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nama"
                android:textSize="24dp"
                android:layout_marginHorizontal="2dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=":"
                android:textSize="24dp"
                android:layout_marginHorizontal="2dp"/>

            <TextView
                android:id="@+id/tv_un_nama"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="24dp"
                android:textAlignment="center"
                android:layout_marginHorizontal="3dp"/>

        </LinearLayout><LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginVertical="3dp"
            android:layout_marginHorizontal="4dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NISN"
                android:textSize="24dp"
                android:layout_marginHorizontal="2dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=":"
                android:textSize="24dp"
                android:layout_marginHorizontal="2dp"/>

            <TextView
                android:id="@+id/tv_un_nisn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="24dp"
                android:textAlignment="center"
                android:layout_marginHorizontal="3dp"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginVertical="3dp"
            android:layout_marginHorizontal="4dp">

            <TextView
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:text="Matematika"
                android:textSize="24dp"
                android:layout_marginHorizontal="2dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=":"
                android:textSize="24dp"
                android:layout_marginHorizontal="2dp"/>

            <TextView
                android:id="@+id/tv_un_math"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="24dp"
                android:textAlignment="center"
                android:layout_marginHorizontal="3dp"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginVertical="3dp"
            android:layout_marginHorizontal="4dp">

            <TextView
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:text="Bahasa Indonesia"
                android:textSize="24dp"
                android:layout_marginHorizontal="2dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=":"
                android:textSize="24dp"
                android:layout_marginHorizontal="2dp"/>

            <TextView
                android:id="@+id/tv_un_indo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="24dp"
                android:textAlignment="center"
                android:layout_marginHorizontal="3dp"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginVertical="3dp"
            android:layout_marginHorizontal="4dp">

            <TextView
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:text="English"
                android:textSize="24dp"
                android:layout_marginHorizontal="2dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=":"
                android:textSize="24dp"
                android:layout_marginHorizontal="2dp"/>

            <TextView
                android:id="@+id/tv_un_english"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="24dp"
                android:textAlignment="center"
                android:layout_marginHorizontal="3dp"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginVertical="3dp"
            android:layout_marginHorizontal="4dp">

            <TextView
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:text="Kejuruan"
                android:textSize="24dp"
                android:layout_marginHorizontal="2dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=":"
                android:textSize="24dp"
                android:layout_marginHorizontal="2dp"/>

            <TextView
                android:id="@+id/tv_un_kejuruan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="24dp"
                android:textAlignment="center"
                android:layout_marginHorizontal="3dp"/>

        </LinearLayout>


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>